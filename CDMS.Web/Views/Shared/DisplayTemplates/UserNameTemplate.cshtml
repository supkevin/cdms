@model string

@{
    SelectListItem info = ((SelectList)ViewBag.UserList).Where(x => x.Value == Model).SingleOrDefault();
}
<!---->
<input class="form-control auto-complete-name text-box single-line" 
       id="txtUserName" name="txtUserName" 
       readOnly="ReadOnly" required="required" type="text" value="@( info == null ? "" : info.Text)" />  