@model string

@{
    SelectListItem info = ((SelectList)ViewBag.ProductList).Where(x => x.Value == Model).SingleOrDefault();
}
<input class="form-control auto-complete-name text-box single-line" 
       id="txtProductName" name="txtProductName" 
       readOnly="ReadOnly" required="required" type="text" value="@( info == null ? "" : info.Text)" />  